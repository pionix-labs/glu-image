{
    "name": "pionix-labs/glu-image",
    "description": "Laravel 5 support for glu-image package",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lukasz Gaszyna",
            "email": "glukash@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "intervention/image": "2.*",
        "sybio/gif-frame-extractor": "1.*",
        "sybio/gif-creator": "1.*",
        "illuminate/support": "4.*|5.*"
    },
    "require-dev":{
        "squizlabs/php_codesniffer": "^3.3",
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "PionixLabs\\GluImage\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
